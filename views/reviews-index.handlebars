<!-- reviews-index -->
<h1>Reviews!</h1>

<a href="/reviews/new">New Review</a>

{{#each reviews}}
  <h2><a href="/reviews/{{this._id}}">{{this.title}}</a></h2>
  <h2>{{this.title}}</h2>
  <small>{{this.movieTitle}}</small>
  <small>{{this.description}}</small>
  <small>{{this.ratingDropdown}}</small>
{{/each}}
